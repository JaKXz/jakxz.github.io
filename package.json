{
	"name": "jason-kurian-portfolio",
	"private": true,
	"scripts": {
		"build": "vite build",
		"predeploy": "test -z \"$(git status --porcelain)\" && npm run build && touch build/.nojekyll && cp CNAME build",
		"deploy": "npx gh-pages -d build -b main --dotfiles --tag $(git rev-parse --short HEAD)",
